t2.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/data/Provider.js",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/hooks/UseCounter.js",["140"],"import React, {useState} from 'react'; // ou import {useState} from 'react'\n\nconst useCounter = (initialValue = 0) => {\n  let [count, setCount] = useState(initialValue);\n\n  function updateState(value) {\n    setCount(count = +value);\n  }\n\n  function inc() {\n    setCount(count + 1);\n  }\n\n  function dec() {\n    setCount(count - 1);\n  }\n\n  return [count, inc, dec, updateState]; //poderia ter retornado obj também\n};\n\nexport default useCounter;\n","/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/hooks/useFetch.js",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseReducer2.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/Store/index.js",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/Store/Reducers/index.js",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/Store/Reducers/number.js",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/Store/Reducers/user.js",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/Store/Actions/index.js",[],"/media