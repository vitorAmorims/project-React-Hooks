back.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseState.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseEffect.jsx",["100"],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseRef.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseMemo.jsx",["101"],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseReducer.jsx",["102"],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseCustom.jsx",["103"],"import React from 'react'\nimport PageTitle from '../../components/layout/PageTitle';\nimport SectionTitle from \"../../components/layout/SectionTitle\";\nimport useCounter from '../../hooks/UseCounter';\nimport {useFetch} from '../../hooks/useFetch';\n\nconst UseRef = (props) => {\n    const [count, inc, dec, updateState] = useCounter(); //se não quiser determinado valor [count,,]\n    \n    const url = 'http://files.cod3r.com.br/curso-react/estados.json';\n    const response = useFetch(url);\n    const { data } = response;\n    const show = (data) => {\n      return data.map(state => <li key={state.nome}> {`${state.nome} - ${state.sigla}`} </li>)\n    };\n\n    return (\n      <div className=\"UseCustom\">\n        <PageTitle\n          title=\"Seu Hook\"\n          subtitle=\"Vamos aprender como criar o nosso próprio Hook!\"\n        />\n        <SectionTitle title=\"Exercício #01\" />\n        <div className=\"center\">\n          <h3>Usando custom Hooks useCounter</h3>\n          <span className=\"text\">{count}</span>\n          <div className=\"center\">\n            <button className=\"btn\" onClick={() => inc()}>\n              +1\n            </button>\n            <button className=\"btn\" onClick={() => dec()}>\n              -1\n            </button>\n            <input\n              type=\"number\"\n              onChange={(event) => updateState(event.target.value)}\n              placeholder=\"alterar numero inicial\"\n            />\n          </div>\n        </div>\n        <SectionTitle title=\"Exercício #02\" />\n        <div className=\"center\">\n          <h3>Usando custom Hooks useFetch</h3>\n        <ul>\n          {!response.loading ? show(response.data) : false }\n        </ul>\n        </div>\n      </div>\n    );\n}\n\nexport default UseRef\n","/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseContext.jsx",["104"],"import React, { useContext, useEffect } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport DataContext from '../../data/DataContext';\nimport SectionTitle from \"../../components/layout/SectionTitle\";\nimport { AppContext } from '../../data/Store';\n\n\nconst UseContext = (props) => {\n    const context = useContext(DataContext);\n    const { state, setState } = context;\n    // console.log(state);\n    const {number, text, setNumber, setText} = useContext(AppContext)\n    \n    function addNumber(delta) {\n      setState({\n        ...state, //spread\n        number: state.number + delta, //alterando somente a chave de interesse\n      });\n    }\n\n    useEffect(() => {\n      if (number > 1250) {\n        setText('Eita..')\n      }\n    }, [number])\n\n    return (\n      <div className=\"UseContext\">\n        <PageTitle\n          title=\"Hook UseContext\"\n          subtitle=\"Aceita um objeto de contexto e retorna o valor atual do contexto!\"\n        />\n        <SectionTitle title=\"Exercício #01\" />\n        <div className=\"center\">\n          <span className=\"text\">{state.number}</span>\n          <span className=\"text\">{state.text}</span>\n          <div>\n            <button className=\"btn\" onClick={() => addNumber(1)}>\n              +1\n            </button>\n            <button className=\"btn\" onClick={() => addNumber(-1)}>\n              -1\n            </button>\n          </div>\n        </div>\n        <SectionTitle title=\"Exercício #02\" />\n        <div className=\"center\">\n          <span className=\"text\">{number}</span>\n          <span className=\"text\">{text}</span>\n          <div>\n            <button className=\"btn\" onClick={() => setNumber(number -1)}>\n              -1\n            </button>\n            <button className=\"btn\" onClick={() => setNumber(number +1)}>\n              +1\n            </button>\n          </div>\n        </div>\n      </div>\n    );\n}\n\nexport default UseContext;\n\n//aula 1\n//cofiguração de Hook useContext\n\n//criar os dados fora dos componentes, e depois passar os dados à partir de um provider\n\n//Nota: É comum usar contextAPI com useReducer.\n\n// useReducer é uma alternativa ao useState, para os casos de estados mais complexos\n\n//# aula 01\n\n// -----------1- etapa\n\n//src/data/DataContext.js  //criando context\n\n//import React from 'react';\n\n// export const data = {\n// \tnumber: 123,\n// \ttext: 'Context API...'\n// };\n\n// const DataContext = React.createContext(null);\n\n// export default DataContext;\n\n\n//---------------2 - etapa\n// -------------------Podemos usar DataContext em index.js ou App.js\n\n//config arquivo index.js\n\n// import DataContext, {data} from '../data/DataContext';\n\n// React.DOM.render(\n//   <DataContext.Provider value={data}>\n//     <App />\n//   </DataContext.Provider>, document.getElementById('root')\n// )\n\n// ou\n\n//conf