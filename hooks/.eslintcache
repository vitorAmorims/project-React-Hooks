e/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/index.js",[],["133","134"],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/App.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/components/layout/Content.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/components/layout/Menu.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/NotFound.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/Home.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseCallback.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseState.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseEffect.jsx",["135"],"import React, { useEffect, useState } from \"react\";\nimport PageTitle from \"../../components/layout/PageTitle\";\nimport SectionTitle from \"../../components/layout/SectionTitle\";\n\nfunction calcFatorial(n) {\n  if (n < 0) return -1;\n  if (n === 0) return 1;\n  return calcFatorial(n - 1) * n;\n}\n\nfunction parOuImpar(n) {\n  if (n % 2 === 0) {\n    return \"Par\";\n  } else {\n    return \"Impar\";\n  }\n}\n\nconst renderizar = () => {\n  const future = Date.now() + 5000; //consigo simular lag de processamento\n  while(Date.now() < future) {}\n  return false;\n}\n\n\nconst UseEffect = (props) => {\n  const [number, setNumber] = useState(1);\n  const [fatorial, setFatorial] = useState(1);\n  const [status, setStatus] = useState('impar');\n\n  // ex01\n  useEffect(() => {\n    setFatorial(calcFatorial(number));\n  }, [number]);\n\n  useEffect(() => {\n    if (fatorial > 500) document.title = \"Fatorial maior que 500\";\n  }, [fatorial]);\n  //   function fatorial(number) {\n  //     let num = number;\n  //     let fatorial = 1;\n  //     while (num > 0) {\n  //       fatorial *= num;\n  //       num = num - 1;\n  //     }\n  //     return fatorial;\n  //   }\n\n  // ex02\n  useEffect(() => {\n    setStatus(parOuImpar(number));\n  },[number]);\n\n  // useEffect(() => {\n  //   renderizar()\n  // })\n\n\n  return (\n    <div className=\"UseEffect\">\n      <PageTitle\n        title=\"Hook UseEffect\"\n        subtitle=\"Permite executar efeitos colaterais em componentes funcionais!\"\n      />\n      {/* <button disabled={renderizar()}>Renderizar async</button> */}\n      <SectionTitle title=\"Exercício #01\" />\n      <div className=\"center\">\n        <div>\n          <span className=\"text\">Fatorial: </span>\n          <span className=\"text red\">\n            {fatorial === -1 ? \"Não existe\" : fatorial}\n          </span>\n        </div>\n        <input\n          value={number}\n          onChange={(e) => setNumber(e.target.value)}\n          type=\"number\"\n          className=\"input\"\n        />\n        <div>\n          <button onClick={() => setNumber(number + 1)} className=\"btn\">\n            +\n          </button>\n          <button onClick={() => setNumber(number - 1)} className=\"btn\">\n            -\n          </button>\n        </div>\n      </div>\n      <SectionTitle title=\"Exercício #02\" />\n      <div className=\"center\">\n        <div>\n          <span className=\"text\">Status input - </span>\n          <span className=\"text\">{status}</span>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default UseEffect;\n","/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseRef.jsx",[],"/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseMemo.jsx",["136"],"import React, { useState, useMemo,useEffect } from \"react\";\nimport PageTitle from '../../components/layout/PageTitle'\n\nconst sum = (a, b) => {\n    const future = Date.now() + 2000; //consigo simular lag de processamento\n    while(Date.now() < future) {} //lógica enquanto data atual < data futura não realiza soma\n    return a + b;\n}\n\nconst UseMemo = (props) => {\n    const [number1, setNumber1] = useState(0);\n    const [number2, setNumber2] = useState(0);\n    const [number3, setNumber3] = useState(0);\n    \n    // primeira resolução, tem que importar useEffect do react\n    // const [result, setResult] = useState(0);\n    // useEffect(() => {\n    //     setResult(sum(number1, number2));\n    // }, [number1, number2]);\n\n    // segunda resolução\n    const result  = useMemo(() => sum(number1, number2), [number1, number2]);\n\n    return (\n      <div className=\"UseMemo\">\n        <PageTitle\n          title=\"Hook UseMemo\"\n          subtitle=\"Retorna um valor memoizado!\"\n        />\n        \n        <div className=\"center\">\n            <span className=\"text\">{result}</span>\n            <p>implementada lógica assincrona para os dois primeiros inputs</p>\n            <p>Soma dos dois primeiros inputs</p>\n        <input\n          value={number1}\n          onChange={(e) => setNumber1(parseInt(e.target.value))}\n          type=\"number\"\n          className=\"input\"\n        />\n        <input\n          value={number2}\n          onChange={(e) => setNumber2(parseInt(e.target.value))}\n          type=\"number\"\n          className=\"input\"\n        />\n        <input\n          value={number3}\n          onChange={(e) => setNumber3(parseInt(e.target.value))}\n          type=\"number\"\n          className=\"input\"\n        />\n        </div>\n      </div>\n    );\n}\n\nexport default UseMemo\n","/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseReducer.jsx",["137"],"import React, { useReducer } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\n\nconst initialState = {\n  user: null,\n\tcart: [],\n  products: [],\n  estados: [],\n\t//foco\n\tnumber: 0,\n};\n\nfunction reducer(state, action) {\n  // console.log(state);\n    switch (action.type) {\n      case \"add2\":\n        return { ...state, number: +(state.number) + 2 };\n      case \"login\":\n        // return { ...state, user: { name: 'Vitor' } } //podemos não deixar name fixo\n        return { ...state, user: { name: action.name } }; // deixar o nome variável\n      case 'multiplicar':\n        return {...state, number: +(state.number) * 7 };\n      case 'dividir':\n        return {...state, number: +(state.number) / 25 };\n      case 'arredondar':\n        return {...state,  number: parseInt(state.number) };\n      case 'n':\n        return {...state, number: action.n}\n      default:\n        return state;\n    }\n}\n\nconst UseReducer = (props) => {\n    const [state, dispath] = useReducer(reducer, initialState)\n    return (\n      <div className=\"UseReducer\">\n        <PageTitle\n          title=\"Hook UseReducer\"\n          subtitle=\"Uma outra forma de ter estado em componentes funcionais!\"\n        />\n        <div>\n          <span>{state.number}</span>\n          <div>\n            {state.user ? (\n              <span>{state.user.name}</span>\n            ) : (\n              <span>Sem usuário</span>\n            )}\n          </div>\n          <button onClick={() => dispath({ type: \"add2\" })}>+2</button>\n          <button onClick={() => dispath({ type: \"multiplicar\" })}>\n            *7\n          </button>\n          <button onClick={() => dispath({ type: \"dividir\" })}>\n            /25\n          </button>\n          <button onClick={() => dispath({ type: \"arredondar\" })}>\n            Arredondar\n          </button>\n          <button\n            onClick={() => dispath({ type: \"login\" })} // deixei fixo\n            onClick={() => dispath({ type: \"login\", name: \"Lucas\" })} //setando nome Lucas no estado\n          >\n            Login\n          </button>\n          <br></br>\n          <br></br>\n          <div>\n            <label>Inserir um nome</label>\n            <input\n              type=\"text\"\n              placeholder=\"Digite uma palavra\"\n              onChange={(event) =>\n                dispath({ type: \"login\", name: event.target.value })\n              }\n            />\n          </div>\n          <br></br>\n          <div>\n            <label>Alterar valor de n</label>\n            <input\n            type=\"number\"\n            placeholder=\"insira um valor\"\n            onChange={(event) =>\n              dispath({ type: \"n\", n: event.target.value })\n            }\n            \n          />\n          </div>\n        </div>\n      </div>\n    );\n}\n\nexport default UseReducer\n","/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseCustom.jsx",["138"],"import React from 'react'\nimport PageTitle from '../../components/layout/PageTitle';\nimport SectionTitle from \"../../components/layout/SectionTitle\";\nimport useCounter from '../../hooks/UseCounter';\nimport {useFetch} from '../../hooks/useFetch';\n\nconst UseRef = (props) => {\n    const [count, inc, dec, updateState] = useCounter(); //se não quiser determinado valor [count,,]\n    \n    const url = 'http://files.cod3r.com.br/curso-react/estados.json';\n    const response = useFetch(url);\n    const { data } = response;\n    const show = (data) => {\n      return data.map(state => <li key={state.nome}> {`${state.nome} - ${state.sigla}`} </li>)\n    };\n\n    return (\n      <div className=\"UseCustom\">\n        <PageTitle\n          title=\"Seu Hook\"\n          subtitle=\"Vamos aprender como criar o nosso próprio Hook!\"\n        />\n        <SectionTitle title=\"Exercício #01\" />\n        <div className=\"center\">\n          <h3>Usando custom Hooks useCounter</h3>\n          <span className=\"text\">{count}</span>\n          <div className=\"center\">\n            <button className=\"btn\" onClick={() => inc()}>\n              +1\n            </button>\n            <button className=\"btn\" onClick={() => dec()}>\n              -1\n            </button>\n            <input\n              type=\"number\"\n              onChange={(event) => updateState(event.target.value)}\n              placeholder=\"alterar numero inicial\"\n            />\n          </div>\n        </div>\n        <SectionTitle title=\"Exercício #02\" />\n        <div className=\"center\">\n          <h3>Usando custom Hooks useFetch</h3>\n        <ul>\n          {!response.loading ? show(response.data) : false }\n        </ul>\n        </div>\n      </div>\n    );\n}\n\nexport default UseRef\n","/media/vitor/3a3e3500-644b-48e7-80d4-1471c847cc91/home/vitor/Documents/learning/github/trybe/cod3r/React-Hooks/project-React-Hooks/hooks/src/views/examples/UseContext.jsx",["139"],"import React, { useContext, useEffect } from 'react'\nimport PageTitle from '../../components/layout/PageTitle'\nimport DataContext from '../../data/DataContext';\nimport SectionTitle from \"../../components/layout/SectionTitle\";\nimport { AppContext } from '../../data/Store';\n\n\nconst UseContext = (props) => {\n    const context = useContext(DataContext);\n    const { state, setState } = context;\n    // console.log(state);\n    const {number, text, setNumber, setText} = useContext(AppContext)\n    \n    function addNumber(delta) {\n      setState({\n        ...state, //spread\n        number: state.number + delta, //alterando somente a chave de interesse\n      });\n    }\n\n    useEffect(() => {\n      if (number > 1250) {\n        setText('Eita..')\n      }\n    }, [number])\n\n    return (\n      <div className=\"UseContext\">\n        <PageTitle\n          title=\"Hook UseContext\"\n          subtitle=\"Aceita um objeto de contexto e retorna o valor atual do contexto!\"\n        />\n        <SectionTitle title=\"Exercício #01\" />\n        <div className=\"center\">\n          <span className=\"text\">{state.number}</span>\n          <span className=\"text\">{state.text}</span>\n          <div>\n            <button className=\"btn\" onClick={() => addNumber(1)}>\n              +1\n            </button>\n            <button className=\"btn\" onClick={() => addNumber(-1)}>\n              -1\n            </button>\n          </div>\n        </div>\n        <SectionTitle title=\"Exercício #02\" />\n        <div className=\"center\">\n          <span className=\"text\">{number}</span>\n          <span className=\"text\">{text}</span>\n          <div>\n            <button className=\"btn\" onClick={() => setNumber(number -1)}>\n              -1\n            </button>\n            <button className=\"btn\" onClick={() => setNumber(number +1)}>\n              +1\n            </button>\n          </div>\n        </div>\n      </div>\n    );\n}\n\nexport default UseContext;\n\n//aula 1\n//cofiguração de Hook useContext\n\n//criar os dados fora dos componentes, e depois passar os dados à partir de um provider\n\n//Nota: É comum usar contextAPI com useReducer.\n\n// useReducer é uma alternativa ao useState, para os casos de estados mais complexos\n\n//# aula 01\n\n// -----------1- etapa\n\n//src/data/DataContext.js  //criando context\n\n//import React from 'react';\n\n// export co